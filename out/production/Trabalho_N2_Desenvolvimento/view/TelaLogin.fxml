<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="bgpane" stylesheets="@TelaLogin.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controle.TelaLoginController">
   <children>
      <TextField fx:id="txtLogin" layoutX="181.0" layoutY="137.0" prefHeight="25.0" prefWidth="239.0" promptText="Login" styleClass="textfield" stylesheets="@TelaLogin.css">
         <font>
            <Font size="18.0" />
         </font></TextField>
      <Label alignment="TOP_CENTER" layoutX="212.0" layoutY="50.0" prefHeight="55.0" prefWidth="176.0" styleClass="loginlabel" stylesheets="@TelaLogin.css" text="Login" textFill="BLACK">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <PasswordField fx:id="txtSenha" layoutX="181.0" layoutY="189.0" prefHeight="25.0" prefWidth="239.0" promptText="Senha" styleClass="passwordfield" stylesheets="@TelaLogin.css">
         <font>
            <Font size="18.0" />
         </font></PasswordField>
      <Button fx:id="btnEntrar" layoutX="181.0" layoutY="255.0" mnemonicParsing="false" onAction="#btnEntrarOnAction" prefHeight="36.0" prefWidth="239.0" styleClass="buttons" stylesheets="@TelaLogin.css" text="Entrar" textFill="BLACK" />
   </children>
</AnchorPane>
